using System;
namespace Cuadratica
{
    public class Punto
    {
        int x;
        int y;

        public Punto()
        {
            x = y = 0;
        }

        public Punto(int xi, int yi)
        {
            x = xi;
            y = yi;
        }

        public int X
        {
            get { return x; }
            set { x = value; }
        }

        public int Y
        {
            get { return y; }
            set { y = value; }
        }
    }
}
